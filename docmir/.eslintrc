{
  "extends": [
    "oclif",
    "oclif-typescript"
  ],
  "rules": {
    "unicorn/filename-case": "off",
    "@typescript-eslint/ban-ts-ignore":  "off",
    "@typescript-eslint/no-unused-vars": "off",
    "no-console": "off",
    "unicorn/no-process-exit": "off",
    "no-process-exit": "off"
  }
}
