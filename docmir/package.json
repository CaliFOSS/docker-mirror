{
  "name": "docmir",
  "description": "A CLI application to create and sync images from dockerhub to provider of choice. Currently only supports (AWS ECR)",
  "version": "0.6.2",
  "author": "jeffgarrett @JeffreyGarrett",
  "bin": {
    "docmir": "./bin/run"
  },
  "bugs": "https://github.com/CaliFOSS/docker-mirror/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "atob": "^2.1.2",
    "aws-sdk": "^2.936.0",
    "axios": "^0.21.1",
    "docker-cli-js": "^2.8.0",
    "sinon": "^11.1.1",
    "tslib": "^1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10.17.60",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "jest": "^27.0.6",
    "mocha": "^5",
    "nyc": "^14",
    "ts-mock-imports": "^1.3.7",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/CaliFOSS/docker-mirror",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "docmir",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "CaliFOSS/docker-mirror",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "docmir": "./bin/run"
  },
  "types": "lib/index.d.ts"
}
